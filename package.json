{
	"name": "sheets-sorcery",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"dev": "npx serve docs -p 3333",
		"lint": "biome check . && knip",
		"a11y:serve": "serve docs -p 3333",
		"a11y:check": "axe http://localhost:3333/index.html --tags wcag2a,wcag2aa --exit",
		"a11y:check:all": "start-server-and-test \"pnpm a11y:serve\" http://localhost:3333 \"pnpm a11y:check\"",
		"lint:fix": "biome check --write .",
		"knip": "knip",
		"prepare": "node tools/setup-hooks.js",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage"
	},
	"devDependencies": {
		"@axe-core/cli": "^4.10.2",
		"@biomejs/biome": "^2.3.13",
		"@vitest/coverage-v8": "^4.0.18",
		"knip": "^5.83.0",
		"serve": "^14.2.4",
		"start-server-and-test": "^2.0.10",
		"vitest": "^4.0.18"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"chromedriver",
			"esbuild"
		]
	},
	"packageManager": "pnpm@10.28.2+sha512.41872f037ad22f7348e3b1debbaf7e867cfd448f2726d9cf74c08f19507c31d2c8e7a11525b983febc2df640b5438dee6023ebb1f84ed43cc2d654d2bc326264"
}
